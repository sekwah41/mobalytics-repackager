{
  "name": "mobalinux-repackager",
  "version": "1.0.0",
  "description": "Mobalytics tool for Linux",
  "main": "src/extract.js",
  "repository": "https://github.com/sekwah41/mobalinux",
  "author": "Sekwah",
  "license": "MIT",
  "private": true,
  "scripts": {
    "extract": "node src/extract.js",
    "postextract": "electron-builder install-app-deps",
    "dist": "electron-builder --config electron-builder/config.js",
    "generate-patches": "diff -uarN b a > patches.diff",
    "create-patched-src": "rm -rf modified-src && cp -R app/electron-src modified-src && cd modified-src && patch -i ../patch.diff",
    "create-patch": "diff -uraN app/electron-src modified-src > patch.diff"
  },
  "dependencies": {
    "asar": "^3.1.0",
    "node-7z": "^3.0.0",
    "progress": "^2.0.3",
    "progress-download": "^1.0.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "electron": "^13.5.1",
    "electron-builder": "^22.13.1"
  }
}
